{
  "name": "loopback-testing",
  "version": "0.0.1",
  "description": "Unit Testing for loopback",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "16 || 18 || 20"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean *example-todo*.tgz dist *.tsbuildinfo package",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "precoverage": "npm test",
    "coverage": "open coverage/index.html",
    "coverage:ci": "lb-nyc report --reporter=text-lcov | coveralls",
    "test": "lb-nyc npm run mocha",
    "test:ci": "lb-nyc npm run mocha",
    "mocha": "lb-mocha \"dist/__tests__/**/*.js\"",
    "mocha:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "verify": "npm pack && tar xf loopback-todo*.tgz && tree package && npm run clean",
    "premigrate": "npm run build ",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "node ."
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Jaranchai Nut <jaranchai.nt@hotmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^6.1.1",
    "@loopback/core": "^5.1.1",
    "@loopback/http-caching-proxy": "^5.1.1",
    "@loopback/repository": "^6.1.1",
    "@loopback/rest": "^13.1.1",
    "@loopback/rest-explorer": "^6.1.1",
    "@loopback/service-proxy": "^6.1.1",
    "axios": "^1.4.0",
    "lodash": "^4.17.21",
    "loopback-connector-postgresql": "^5.5.2",
    "loopback-connector-rest": "^3.7.0",
    "mocha": "^10.2.0",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^10.1.1",
    "@loopback/eslint-config": "^14.0.2",
    "@loopback/testlab": "^6.1.1",
    "@types/lodash": "^4.14.196",
    "@types/morgan": "^1.9.4",
    "@types/node": "^16.18.38",
    "eslint": "^8.45.0",
    "source-map-support": "^0.5.21",
    "typescript": "~5.1.6"
  }
}
